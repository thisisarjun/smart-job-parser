# Environment configuration for Smart Job Parser API
# Copy this file to .env for local development

# Application settings
PYTHONPATH=/app
PORT=8000

# API Configuration
FASTAPI_ENV=development
DEBUG=true

# CORS settings (for development - restrict in production)
ALLOWED_ORIGINS=["http://localhost:3000", "http://localhost:8080", "http://127.0.0.1:3000"]

# LangChain/AI Configuration
# OPENAI_API_KEY=your_openai_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Database Configuration (if needed)
# DATABASE_URL=postgresql://user:password@localhost/smart_job_parser

# Logging
LOG_LEVEL=INFO

PINECONE_API_KEY = "asdasdasdasdasd" # pragma: allowlist secret
PINECONE_INDEX = "idx"
PINECONE_NAMESPACE = "dummy"
